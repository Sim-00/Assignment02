{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": 300,
    "data":{
        "url": "../Datasets/purposeInternet.csv"
    },
    "mark": "arc",
    "encoding": {
        "theta":{
            "field": "Percentage",
            "type":"quantitative"
        },
        "color":{
            "field": "Purpose",
            "type": "nominal"
        },
        "tooltip": [
            {
                "field": "Purpose",
                "type": "nominal",
                "title": "Purpose: "
            },
            {
                "field": "Percentage",
                "type": "quantitative",
                "title": "Percentage: "
            }
        ]
    },
    "config": {
        "background":"transparent"
    }
}