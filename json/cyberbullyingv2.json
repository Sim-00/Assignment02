{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": 500,
    "title": {"text": "Percentage of Children as Cyberbully Victim"},
    "data": {
        "url": "../Datasets/cyberbullying.csv"
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "Percentage",
            "type": "quantitative"
        },
        "y":{
            "field": "Country",
            "type": "nominal"
        }
    },
    "config": {
        "background": "transparent",
        "axis": {
            "grid": false
        }
    }
}